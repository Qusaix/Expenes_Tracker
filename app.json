    },
    
    "assetBundlePatterns": [
      "**/*"
    ],

    "android": {
      "package": "com.israr.expensesTracker",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-6984928475835712~5090402042"
      }

    }

  }
}
